<!DOCTYPE html>
<html>
<head>
  <meta charset="utf-8">
  <title>Maestro Server - Cloud Inventory</title>
  <meta name="Description" content="Graph viewer - Maestro Server">

  <link rel="icon" type="image/png" sizes="16x16" href="/static/imgs/favicon.ico">

  <meta name="viewport" content="width=device-width, initial-scale=1.0">

  <link rel="stylesheet" type="text/css" href="{hostname}/css/main.css"></link>
  <meta name="api_url" content="{api_url}">
  <meta name="id" content="{graph_id}">

</head>
<body>
    <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink">
        <filter id="glow" height="300%" width="300%" x="-75%" y="-75%">
            <feMorphology operator="dilate" radius="4" in="SourceAlpha" result="thicken" />
            <feGaussianBlur in="thicken" stdDeviation="10" result="blurred" />
            <feFlood flood-color="rgb(234,25,25)" result="glowColor" />
            <feComposite in="glowColor" in2="blurred" operator="in" result="softGlow_colored" />
            <feMerge>
                <feMergeNode in="softGlow_colored"/>
                <feMergeNode in="SourceGraphic"/>
            </feMerge>
        </filter>
    </svg>

    <div id="viewer">
        {payload|s}
    </div>

    <div id="info" class="info_box flex">
        <ul class="menu">
            <li class="title">
                <h1></h1>
            </li>
            <li class="histograms">
                <h2>Histogram</h2>
                <div class="hist"></div>
            </li>
            <li class="info">
                <h2>Info</h2>
                <ol></ol>
            </li>
            <li class="families">
                <h2>Types</h2>
                <ol></ol>
            </li>
            <li class="system">
                <h2>Systems <span class="number">(2)</span></h2>
                <ul></ul>
            </li>
            <li class="clients">
                <h2>Clients <span class="number">(1)</span></h2>
                <ul></ul>
            </li>
        </ul>

        <a href="http://maestroserver.io" target="_blank" class="footer">Powered by<br/> Maestro Server</a>
    </div>

    <div id="infobox" class="info_box"></div>

    <div id="conn-tooltip" class="minitlp noshow"></div>

    <div id="app-tooltip" class="apptlp noshow">
        <ul></ul>
        <div class="tb-btn">
            <a href="#more">></a>
        </div>
    </div>

    <script id="tpl_clients" type="text/template">
        <div class="wrapper flex">
            <div class="half">
                <h1>{~lb}{~lb}name{~rb}{~rb}</h1>

                <ul>
                    <li>
                        <p>{~lb}{~lb}description{~rb}{~rb}</p>
                    </li>
                    <li>
                        <h2>Contacts</h2>
                        <ol>
                        {~lb}{~lb}#contacts{~rb}{~rb}
                            <li><b>{~lb}{~lb}channel{~rb}{~rb}:</b> {~lb}{~lb}value{~rb}{~rb}</li>
                        {~lb}{~lb}/contacts{~rb}{~rb}
                        </ol>
                    </li>
                    <li>
                        <h2>Tags</h2>
                        <ol>
                        {~lb}{~lb}#tags{~rb}{~rb}
                            <li><b>{~lb}{~lb}key{~rb}{~rb}:</b> {~lb}{~lb}value{~rb}{~rb}</li>
                        {~lb}{~lb}/tags{~rb}{~rb}
                        </ol>
                    </li>
                    <li>
                        <h2>Owner</h2>
                        <p>{~lb}{~lb}owner.email{~rb}{~rb}</p>
                    </li>
                    <li>
                        <h2>Update at</h2>
                        <p>{~lb}{~lb}updated_at{~rb}{~rb}</p>
                    </li>
                    <li>
                        <h2>Create at</h2>
                        <p>{~lb}{~lb}created_at{~rb}{~rb}</p>
                    </li>
                </ul>
            </div>
        </div>
    </script>

    <script src="{hostname}/js/components/jquery/dist/jquery.min.js"></script>
    <script src="{hostname}/js/components/svg.js/dist/svg.min.js"></script>
    <script src="{hostname}/js/components/svg-pan-zoom/dist/svg-pan-zoom.min.js"></script>
    <script src="{hostname}/js/components/lodash/dist/lodash.min.js"></script>
    <script src="{hostname}/js/components/mustache.js/mustache.min.js"></script>
    <script src="{hostname}/js/main.js"></script>

</body>
</html>